config interfaceLimit 'wan1_limit'
        option interface 'eth1'
        option interfaceNick 'WAN1'
        option enabled '1'
        option upload_limit '512'
        option download_limit '1024'
        option threshold '10000'
		option alert_enabled '1'
		
config interfaceLimit 'wan2_limit'
        option interface 'eth2'
        option interfaceNick 'WAN2'
        option enabled '0'
        option upload_limit '512'
        option download_limit '1024'
        option threshold '1000000'
        option alert_enabled '1'
        
config interfaceLimit 'wan3_limit'
        option interface 'eth3'
        option interfaceNick 'WAN3'
        option enabled '0'
        option upload_limit '512'
        option download_limit '1024'
        option threshold '1000000'
        option alert_enabled '1'

config service 'service'
        option refreshrate '120'
		option mailaddress 'timuni@hanika.org'
		option sourcename 'Hantec GmbH network test'